{
	"name": "my-monorepo",
	"version": "0.0.1",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"description": "My Monorepo template",
	"main": "index.js",
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev --parallel",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\""
	},
	"devDependencies": {
		"eslint-config-custom": "workspace:*",
		"prettier": "^2.7.1",
		"turbo": "^1.6.3"
	},
	"keywords": [],
	"author": "",
	"license": "ISC"
}
